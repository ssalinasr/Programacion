Algoritmo sin_titulo
	definir rad1,rad2,radtot como real;
	definir volcsq como real;
	
	escribir "radio esfera 1:";
	leer rad1;
	
	escribir "radio esfera 2:";
	leer rad2;
	
	radtot<-(rad2)^3-(rad1)^3;
	volcsq<-(4/3)*radtot;
	
	escribir "el volumen del casquete entre las dos esferas es:";
	escribir volcsq;
	
	
FinAlgoritmo
